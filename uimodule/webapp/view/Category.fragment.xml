<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core">
    <List
      id="categoryList"
      mode="None"
      busyIndicatorDelay="0"
      items="{category>/}">
      <items>
        <StandardListItem
          title="{category>CategoryName}"
          type="Active"
          press=".onCategoryListItemPress"
          tooltip="{i18n>openCategoryProducts} {CategoryName}">
          <customData>
            <core:CustomData
              key="id"
              value="{Category}"/>
          </customData>
        </StandardListItem>
      </items>
    </List>
